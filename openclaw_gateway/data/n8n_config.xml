<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="config_n8n_base_url" model="ir.config_parameter">
            <field name="key">openclaw_gateway.n8n_base_url</field>
            <field name="value">http://localhost:5678</field>
        </record>
        <record id="config_webhook_secret" model="ir.config_parameter">
            <field name="key">openclaw_gateway.webhook_secret</field>
            <field name="value"></field>
        </record>
        <record id="config_n8n_api_key" model="ir.config_parameter">
            <field name="key">openclaw_gateway.n8n_api_key</field>
            <field name="value"></field>
        </record>
        <record id="config_max_bulk_size" model="ir.config_parameter">
            <field name="key">openclaw_gateway.max_bulk_import_size</field>
            <field name="value">1000</field>
        </record>
    </data>
</odoo>
